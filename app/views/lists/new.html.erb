 <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Create a new list</h1>
      <%= simple_form_for @list do |f| %>
        <%= f.input :name %>
        <%= f.submit class: 'btn btn-primary m-2' %>
      <% end %>
      <%= link_to "Back", lists_path, class: 'btn btn-secondary m-2' %>
    </div>
  </div>
</div>
